+int cfe_commit(void)
+{
+        DECLARE_WAITQUEUE(wait, current);
+        wait_queue_head_t wait_q;
+        struct erase_info erase;
+//        unsigned int i;
+        int ret;
+        size_t erasesize, len;
+        u_int32_t offset;
+//        char *buf;
